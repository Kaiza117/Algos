}

// 4
function multiples(){
    for(i=-300; i >0; i++){
        if (i = -3 || -6){
            i=i
        }
        else if(i % 3==0){
            console.log(i)
        }
    }
}

// 5
function womp(){
    x = 2000
    while(x < 5280){
        console.log(x)
        x++
    }
}

// 6
function bday(x,y){
    if (x == 3 && y == 15){
        console.log("How did you know?")
    }
    else if (x == 15 && y == 3){
        console.log("How did you know?")
    }
    else {
        console.log("Just another day...")
    }
}

console.log(bday(3,15))

// 7
function leapYear(){
    if(x % 4 == 0 && x % 100 != 0){
        console.log("Leap Year!")
    }
    else if(x % 400 == 0) {
        console.log("Leap Year!")
    }
    else {
        console.log("Not a Leap Year!")
    }
}

console.log(leapYear())

// 8
function multiplesSix(){
    var x = 0
    while(x<60000){
        x += 6
        console.log(x)
    }
}

console.log (multiplesSix())

// 9
function codingDojo(){
    for (i = 1; i < 100; i++){
        if (x % 5 == 0){
            console.log("Coding")
            if (i % 10 == 0){
                console.log("Dojo!")
            }
        }
    }
}

