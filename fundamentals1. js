// 11
function huge(){
    var x = -300000
    while(x < 300000){
        if(x % 2 != 0){
            sum += x
        }
    }
    console.log(sum)
}

// 12
function countdown(){
    x = 2016
    while(x > 0){
        console.log(x)
        x - 4
    }
}

// 13
function finalCountdown(a,b,c,d){
    while(b < c){
        if(b % a == 0 && b != d){
            console.log(b)
        }
    b++;
    }
}

console.log(finalCountdown(3,5,17,9));